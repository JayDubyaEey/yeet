{
  "keyVaultName": "my-keyvault-name",
  "mappings": {
    "DATABASE_URL": {
      "local": {
        "type": "keyvault",
        "value": "postgres-connection-string"
      },
      "docker": {
        "type": "keyvault", 
        "value": "postgres-docker-connection-string"
      }
    },
    "REDIS_URL": {
      "local": {
        "type": "keyvault",
        "value": "redis-connection-string"
      },
      "docker": {
        "type": "literal",
        "value": "redis://redis:6379"
      }
    },
    "API_KEY": {
      "local": {
        "type": "keyvault",
        "value": "api-key"
      },
      "docker": {
        "type": "keyvault",
        "value": "api-key"
      }
    },
    "JWT_SECRET": {
      "local": {
        "type": "literal",
        "value": "local-dev-secret-123"
      },
      "docker": {
        "type": "keyvault",
        "value": "jwt-secret-production"
      }
    },
    "LOG_LEVEL": {
      "local": {
        "type": "literal",
        "value": "debug"
      },
      "docker": {
        "type": "literal",
        "value": "info"
      }
    },
    "PORT": {
      "type": "literal",
      "value": "8080"
    },
    "SIMPLE_VAR": "simple-keyvault-secret"
  }
}
